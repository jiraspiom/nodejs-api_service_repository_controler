// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  name     String
  email    String @unique
  password String
}

model Lancamento {
  id         Int       @id @default(autoincrement())
  data       DateTime  @db.Date
  operacao   String
  ativo      String
  setor      String
  quantidade Int
  preco      Decimal
  create_at  DateTime
  update_at  DateTime?
}
